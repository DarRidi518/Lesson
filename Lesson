#Печерин Егор ИИ-71, задания за 06.12.2025

#Задача №1
nums = int(input("Кол-во чисел: "))
nums_list = []
count = 0
for i in range(nums):
    numbers = int(input("Введите число: "))
    nums_list.append(numbers)

pairs = [a * b for a in nums_list for b in nums_list]

for num in pairs:
    if num % 26:
        count += 1
print(count // 2)

#Задача №2
nums = int(input("Кол-во чисел: "))
nums_list = []
count = 0
for i in range(nums):
    numbers = int(input("Введите число: "))
    nums_list.append(numbers)

pairs = [a * b for a in nums_list for b in nums_list]

for num in pairs:
    if num % 62:
        count += 1
print(count // 4)

